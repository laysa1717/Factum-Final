<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.factum.app">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PrincipalActivity"
            android:label="@string/title_activity_principal"
            android:parentActivityName=".PrincipalActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.com.factum.app.MainActivity" />
        </activity>
        <activity
            android:name=".MaterialActivity"
            android:label="@string/title_activity_material"
            android:parentActivityName=".PrincipalActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.com.factum.app.PrincipalActivity" />
        </activity>
        <activity
            android:name=".NovoMaterialActivity"
            android:label="@string/title_activity_novo_material"
            android:parentActivityName=".MaterialActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.com.factum.app.MaterialActivity" />
        </activity>
        <activity
            android:name=".EscolherSimuladoActivity"
            android:label="@string/title_activity_escolher_simulado"
            android:parentActivityName=".PrincipalActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.com.factum.app.PrincipalActivity" />
        </activity>
        <activity
            android:name=".ResultadoActivity"
            android:label="@string/title_activity_resultado"
            android:parentActivityName=".PrincipalActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.com.factum.app.PrincipalActivity" />
        </activity>
        <activity
            android:name=".QuestoesActivity"
            android:label="@string/title_activity_questoes"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.com.factum.app.PrincipalActivity" />
        </activity>
        <activity
            android:name=".DetalhesMaterialActivity"
            android:label="@string/title_activity_detalhes_material"
            android:parentActivityName=".MaterialActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.com.factum.app.MaterialActivity" />
        </activity>
        <activity
            android:name=".EditarMaterialActivity"
            android:label="@string/title_activity_editar_material"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EstudoActivity"
            android:label="@string/title_activity_estudo"
            android:parentActivityName=".PrincipalActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.com.factum.app.PrincipalActivity" />
        </activity>
        <activity
            android:name=".CadastroEstudoActivity"
            android:label="@string/title_activity_cadastro_estudo"
            android:parentActivityName=".EstudoActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.com.factum.app.PrincipalActivity" />
        </activity>
        <activity
            android:name=".DetalhesEstudoActivity"
            android:label="@string/title_activity_detalhes_estudo"
            android:parentActivityName=".EstudoActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.com.factum.app.PrincipalActivity" />
        </activity>
        <activity
            android:name=".EditarEstudoActivity"
            android:label="@string/title_activity_editar_estudo"
            android:parentActivityName=".DetalhesEstudoActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.com.factum.app.PrincipalActivity" />
        </activity>
        <activity
            android:name=".PendentesActivity"
            android:label="@string/title_activity_pendentes"
            android:parentActivityName=".PrincipalActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="br.com.factum.app.PrincipalActivity" />
        </activity>
    </application>

</manifest>